function [pred_y,true_y] = svmClassifier(data_train,data_test,kernelFunction,c,)
%SVMCLASSIFIER Summary of this function goes here
%   Detailed explanation goes here
model = fitcsvm(data_train.X',data_train.y','KernelFunction','Linear','BoxConstraint',c,'Solver','SMO');
[ypred]=predict(model,data_test.X')';

end

